<?xml version="1.0" encoding="utf-8"?>
<!--Inherited the model hr.payslip to show the GOSI Reference in the payslip-->
<odoo>
    <record id="hr_payslip_view_form" model="ir.ui.view">
        <field name="name">hr.payslip.view.form.inherit.saudi.gosi</field>
        <field name="model">hr.payslip</field>
        <field name="inherit_id"
               ref="hr_payroll_community.hr_payslip_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='struct_id']" position="after">
                <field name="gosi_no_id"/>
            </xpath>
        </field>
    </record>
</odoo>
